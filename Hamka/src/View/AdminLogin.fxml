<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="236.0" prefWidth="452.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.AdminController">
   <children>
      <ImageView fitHeight="276.0" fitWidth="454.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/adminBackground.jpg" />
         </image>
      </ImageView>
      <PasswordField fx:id="pass" layoutX="136.0" layoutY="128.0" prefHeight="32.0" prefWidth="179.0" promptText="Enter your password" style="-fx-background-radius: 30;" />
      <Button fx:id="login" layoutX="194.0" layoutY="172.0" mnemonicParsing="false" onAction="#login" style="-fx-background-radius: 30; -fx-background-color: transparent;" text="Login" textFill="WHITE">
         <font>
            <Font name="Berlin Sans FB" size="17.0" />
         </font>
      </Button>
      <TextField fx:id="userName" layoutX="138.0" layoutY="77.0" prefHeight="32.0" prefWidth="179.0" promptText="Enter userName" style="-fx-background-radius: 30;" />
      <ImageView fitHeight="47.0" fitWidth="47.0" layoutX="203.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/icons8-lock-480.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
