<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="450.0" prefWidth="456.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.QuestionController">
   <children>
      <ImageView fx:id="image" fitHeight="98.0" fitWidth="104.0" layoutX="154.0" layoutY="65.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../images/ques.jpg" />
         </image>
      </ImageView>
      <Button fx:id="back" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="21.0" prefWidth="57.0" style="-fx-background-radius: 30;" text="back" textFill="#cd4f06">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <HBox layoutX="7.0" layoutY="199.0" prefHeight="34.0" prefWidth="440.0" spacing="25.0">
         <children>
            <Button fx:id="add" mnemonicParsing="false" prefHeight="29.0" prefWidth="155.0" style="-fx-background-radius: 30;" text="Add Question" textFill="#cd4f06">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="update" mnemonicParsing="false" prefHeight="29.0" prefWidth="155.0" style="-fx-background-radius: 30;" text="Update Question" textFill="#cd4f06">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
            <Button fx:id="delete" mnemonicParsing="false" prefHeight="29.0" prefWidth="155.0" style="-fx-background-radius: 30;" text="Delete Question" textFill="#cd4f06">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Button>
         </children>
      </HBox>
      <ListView fx:id="list" layoutX="24.0" layoutY="251.0" prefHeight="163.0" prefWidth="398.0" />
   </children>
</AnchorPane>
