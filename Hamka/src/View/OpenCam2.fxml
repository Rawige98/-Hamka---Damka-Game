<AnchorPane prefHeight="333.0" prefWidth="500.0" style="-fx-background-color: #59a6da;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.WebCamPreviewController">
