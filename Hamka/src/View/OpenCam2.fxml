      <Button fx:id="btnProceedCamera" focusTraversable="false" layoutX="301.0" layoutY="281.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#proceed2" prefHeight="34.0" prefWidth="97.0" style="-fx-background-radius: 30; -fx-background-color: white; -fx-border-radius: 30; -fx-border-color: black;" text="Proceed">
